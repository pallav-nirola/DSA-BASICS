#include <iostream>
#include <queue>
using namespace std;

void quu(queue<int> &q1)
{
    int n = q1.size();

    int half = n / 2;
    queue<int> q2;

    if (n % 2 != 0)
    {
        cout << "there must be an even numbers of queue" << endl;
        return;
    }
    else
    {

        for (int i = 0; i < half; i++)
        {
            q2.push(q1.front());
            q1.pop();
        }
    }
    while (q2.empty() == 0)
    {
        q1.push(q2.front());
        q2.pop();
        q1.push(q1.front());
        q1.pop();
    }
}
void display(queue<int> copy)
{
    while (copy.empty() == 0)
    {

        cout << copy.front() << " ";
        copy.pop();
    }
}

int main()
{

    queue<int> qu;
    qu.push(4);
    qu.push(7);
    qu.push(11);
    qu.push(20);
    qu.push(5);
    qu.push(9);
    quu(qu);
    display(qu);

    return 0;
}
