#include <iostream>
using namespace std;
int main()
{
  int array[4] = {1, 2, 3, 4};
  int output[4];
  int product = 1;
  for (int i = 0; i < 4; i++)
  {
    product = product * array[i];
  }

  for (int i = 0; i < 4; i++)
  {
    output[i] = product / array[i];
  }
  cout << "The output array is ";
  for (int i = 0; i < 4; i++)
  {
    cout << output[i] << " ";
  }

  return 0;
}
