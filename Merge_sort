#include <iostream>
#include <vector>
using namespace std;
void merge(int arambh, int ant, int madhya, vector<int> &vi)
{
    int i = arambh;
    int j = madhya + 1;
    vector<int> temp;
    while (i <= madhya && j <= ant)
    {
        if (vi[i] <= vi[j])
        {
            temp.push_back(vi[i]);
            i++;
        }
        else
        {
            temp.push_back(vi[j]);
            j++;
        }
    }
    while (i <= madhya)
    {
       temp.push_back(vi[i]);
       i++; 
    }
    while (j <= ant)
    {
        temp.push_back(vi[j]);
        j++;
    }
    for (int i = 0; i < temp.size(); i++)
    {
      vi[arambh + i] = temp[i];
    }
    
}
void mergeSort(int start, int end, vector<int> &vi)
{
    if (start >= end)
    {
        return;
    }
    int mid = start + (end - start) / 2;
    mergeSort(start, mid, vi);
    mergeSort(mid + 1, end, vi);
    merge(start, end, mid, vi);
}
int main()
{
    vector<int> v = {9, 4, 1, 7, 5, 3};

    mergeSort(0, v.size() - 1, v);
    for ( int i : v)
    {
        cout<<i<<" ";
    }
    
     return 0;
}
