   Node *merge2Lists(Node *h1, Node *h2)
    {
        if (h1 == NULL)
            return h2;
        if (h2 == NULL)
            return h1;
        if ((*h1).data <= (*h2).data)
        {
            
            (*h1).next = merge2Lists((*h1).next, h2);
            return h1;
        }
        if ((*h1).data > (*h2).data)
        {
            
            (*h2).next = merge2Lists(h1, (*h2).next);
            return h2;
        }
    }
