#include <iostream>
#include <stack>
#include <vector>
using namespace std;
int main()
{
    vector<int> array = {4, 5, 2, 25};
    stack<int> stk;
    vector<int> ans(array.size(), -1);
    for (int i = array.size() - 1; i >= 0; i--)
    {
        if (stk.empty())
        {
            stk.push(array[i]);
        }
        if (!stk.empty())
        {
            if (stk.top() > array[i - 1])
            {
                ans[i - 1] = stk.top();
                stk.push(array[i - 1]);
            }
            while (!stk.empty() && stk.top() < array[i])
            {
                stk.pop();
            }
        }
    }
    for (int i = 0; i < ans.size(); i++)
    {
        cout << ans[i] << " ";
    }

    return 0;
}
