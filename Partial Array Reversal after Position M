#include <iostream>
 #include <algorithm>
using namespace std;
int main (){
    
    int n, m;
    cout<<"Enter the size of the array"<<endl;
    cin>>n;
    int arra[n];
    cout<<"Enter the"<<n<<" elements of the array"<<endl;
    for (int i = 0; i < n; i++)
    {
        cin>>arra[i];
    }
    cout<<"enter the value of m after which you wnat to reverse tha array"<<endl;
    cin>>m;
    reverse(arra+m, arra+n);
    for (int i = 0; i < n; i++)
    {
        cout<<arra[i] <<" ";
    }
    
    
    
    
    return 0;
}
