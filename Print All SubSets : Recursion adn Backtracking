 void PrintAllSubArray(vector<int> &arr, vector<int> ans , int level )
    {
        
        if (arr.size() == level)
        {
            for (int i = 0; i < arr.size(); i++)
            {
                cout << arr[i] << " ";
            }
            cout << endl;
            return;
        }
        ans.push_back(arr[level]);
        PrintAllSubArray(arr, ans, level + 1);
        ans.pop_back();
        PrintAllSubArray(arr,ans, level + 1);
    }
