#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
void selectionSort(){
for(int i = 0 ; i<n-1 ; i ++){
int minIdx = i ;
for(int j = i +1 ; j<n ; j++){
if(array[j]<array[i]){
minIdx = j
}
}
swap(array[i] , array[minIdx])
}
}
void bubble_sort(vector<int> &v)
{
    for (int i = 0; i <= v.size() - 2; i++)
    {

        for (int j = 0; j <= (v.size() - 2) - i; j++)
        {
            if (v[j] > v[j + 1])
            {
                swap(v[j], v[j + 1]);
            }
        }
    }
}
void InserttionSort(){
for(int i = 0 ; i<n-1 ; i++){
int j = i + 1 ;
while(j>0 && array[j-1] >array[j]){
    swap(array[j-1],array[j]);
    j--;
}
}
int main()
{
    vector<int> arr = {13, 46, 24, 52, 20, 9};
    print_insertion_sort(arr);
    return 0;
}

int main()
{
    vector<int> vec = {13, 46, 24, 52, 20, 9};
    print_selection_sort(vec);
    bubble_sort(vec);
    return 0;
}
