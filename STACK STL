#include <iostream>
using namespace std;

#include <stack>

int main()
{
    stack<string> s;

    s.push("First");
    s.push("Second");
    s.push("Third");

    cout << "Top element: " << s.top() << std::endl;

    s.pop();

    cout << "Top element after pop: " << s.top() << std::endl;

    cout << "Stack is not empty." << s.empty() << endl;

    cout << "Stack size: " << s.size() << endl;

    return 0;
}
