class solution
{
public:
    stack<int> stk;
    void dfs(int source, int vertex, vector<bool> &visited, vector<vector<int>> &adj)
    {
        visited[source] = true;
        for (int neighbour : adj[source])
        {
            if (visited[neighbour] != true)
            {
                dfs(neighbour, vertex, visited, adj);
            }
        }
        stk.push(source);
    }
    void restOfCall(int vertex, vector<bool> &visited, vector<vector<int>> adj){
        for (int i = 0; i < vertex; i++)
        {
            if (visited[i] != true)
            {
                dfs(i, vertex, visited, adj);
            }
        }
    }
    void GetTopSort(int vertex, vector<vector<int>> &adj)
    {
        vector<bool> visited(vertex, false);
        while (stk.empty() == 0)
        {
            cout << stk.top() << " ";
            stk.pop();
        }
    }   
};
