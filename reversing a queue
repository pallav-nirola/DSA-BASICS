#include <iostream>
#include <queue>
using namespace std;
void reverse(queue<int> &que){
if (que.empty()==1)
{
  return ;
} // how cna i write the code if I were to write the function as int instead of of void
int x = que.front();
reverse(que);
que.push(x);


    
}
void pushh(queue<int> &q1, int val)
{
    q1.push(val);
}
void display(queue<int> &qu)
{
    while (qu.empty() == 0)
    {
        cout << qu.front() << " ";
        qu.pop();
    } 
}
int main()
{
    queue<int> q1;
    pushh(q1, 10);
    pushh(q1, 20);
    pushh(q1, 30);
    pushh(q1, 40);
reverse(q1);
    display(q1);

    return 0;
}
